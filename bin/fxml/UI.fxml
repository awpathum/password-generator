<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="703.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <children>
      <Label layoutX="197.0" layoutY="14.0" prefHeight="53.0" prefWidth="310.0" text="Password Generator" textAlignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="words" layoutX="202.0" layoutY="98.0" prefWidth="300.0" promptText="Enter a Word" />
      <TextField fx:id="numbers" layoutX="202.0" layoutY="161.0" prefWidth="300.0" promptText="Enter a Number" />
      <TextField fx:id="symbols" layoutX="202.0" layoutY="225.0" prefWidth="300.0" promptText="Enter a Symbol Patern" />
      <Button fx:id="generateBtn" layoutX="407.0" layoutY="353.0" mnemonicParsing="false" onAction="#generate" prefWidth="100.0" text="Generate" />
      <Label fx:id="passwordLbl" layoutX="207.0" layoutY="272.0" prefHeight="39.0" prefWidth="310.0" />
      <Button fx:id="resetBtn" layoutX="197.0" layoutY="353.0" mnemonicParsing="false" onAction="#reset" prefWidth="100.0" text="Reset" />
   </children>
</AnchorPane>
